# next a {{{
Given:
  abc (def (ghi) jkl)

Do:
  ^canbmno

Expect:
  abc mno

Given:
  abc [def [ghi] jkl]

Do:
  ^canrmno

Expect:
  abc mno

Given:
  abc {def {ghi} jkl}

Do:
  ^canBmno

Expect:
  abc mno
# }}}

# next i {{{
Given:
  abc (def (ghi) jkl)

Do:
  ^cinbmno

Expect:
  abc (mno)

Given:
  abc [def [ghi] jkl]

Do:
  ^cinrmno

Expect:
  abc [mno]

Given:
  abc {def {ghi} jkl}

Do:
  ^cinBmno

Expect:
  abc {mno}
# }}}

# last a {{{
Given:
  (abc) def

Do:
  $calbghi

Expect:
  ghi def

Given:
  [abc] def

Do:
  $calrghi

Expect:
  ghi def

Given:
  {abc} def

Do:
  $calBghi

Expect:
  ghi def
# }}}

# last i {{{
Given:
  (abc) def

Do:
  $cilbghi

Expect:
  (ghi) def

Given:
  [abc] def

Do:
  $cilrghi

Expect:
  [ghi] def

Given:
  {abc} def

Do:
  $cilBghi

Expect:
  {ghi} def
# }}}

# corner case {{{

# here is what cib does, for reference.
Given:
  abc (
  jkl
  ) ghi

Do:
  $cibjkl

Expect:
  abc (
  jkl
  ) ghi

Given:
  abc (
  def
  ) ghi

Do:
  cinbjkl

Expect:
  abc (jkl) ghi

# }}}

# quotes {{{
Given:
  foo "bar 
    \"baz\"
  "

Do:
  ^can"bat

Expect:
  foo bat

Given:
  foo "
    bar
    \"baz\"
  "

Do:
  ^cin"bat

Expect:
  foo "bat"

Given:
  "bar 
    \"baz\"
  " foo

Do:
  G$cal"bat

Expect:
  bat foo

Given:
  "
    bar
    \"baz\"
  " foo

Do:
  G$cil"bat

Expect:
  "bat" foo
