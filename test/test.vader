# next a {{{
Given:
  abc (def (ghi) jkl)

Do:
  ^canbmno

Expect:
  abc mno

Given:
  abc [def [ghi] jkl]

Do:
  ^canrmno

Expect:
  abc mno

Given:
  abc {def {ghi} jkl}

Do:
  ^canBmno

Expect:
  abc mno
# }}}

# next i {{{
Given:
  abc (def (ghi) jkl)

Do:
  ^cinbmno

Expect:
  abc (mno)

Given:
  abc [def [ghi] jkl]

Do:
  ^cinrmno

Expect:
  abc [mno]

Given:
  abc {def {ghi} jkl}

Do:
  ^cinBmno

Expect:
  abc {mno}
# }}}

